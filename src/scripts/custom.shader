textures/custom/tr_ladder_clean2
{
	qer_editorimage textures/custom/tr_ladder_clean2.tga
	qer_keyword rusted
	qer_keyword metal
	qer_keyword masked
	surfaceParm metal
	surfaceParm fence
	surfaceParm alphashadow
	surfaceParm playerclip
	surfaceParm monsterclip
	surfaceParm nonsolid
	cull none
	nopicmip
	{
		map textures/custom/tr_ladder_clean2.tga
		alphaFunc GE128
		depthWrite
	nextbundle
		map $lightmap
	}
}

